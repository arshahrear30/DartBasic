class Father{ // Main/parent class 
    
    void display(){
        
        print("My");
        print("is");
        print(58+74);
        
    }

}

class Student extends Father {    //child class 
    //override dilay same name er method 2 bar bebeohar kora jay ..ekon kintu over right korar karon e display name e notun akta method call hoicay .. shey kintu ager method er data gula paibo na .. she akon notun er moto 
    @override 
    void display(){
        super.display();//super diya onno  calss er data access kora jay .. purbay amra this. porcilam . shey this cilo nijer class er bitor e onno method er data access kora .. 
        print("Shahrear");
    }

    



}

void main(){
    
    Student ob  = Student();
    ob.display();
   
}
